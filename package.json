{
  "name": "opdracht",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/jaspervd18/backend_website_trainers.git",
  "author": "Jasper Van Dyck <jasper.vandyck@student.hogent.be>",
  "license": "MIT",
  "jest": {
    "testPathIgnorePatterns": [
      "src",
      "node_modules",
      "config"
    ]
  },
  "scripts": {
    "start": "npx env-cmd nodemon",
    "web": "node src/index.js",
    "test": "npx env-cmd -f .env.test jest",
    "test:coverage": "yarn test --coverage"
  },
  "nodemonConfig": {
    "signal": "SIGTERM",
    "ignore": [
      "node_modules/*",
      ".vscode/*"
    ],
    "delay": "80",
    "watch": [
      "config",
      "src"
    ],
    "ext": "js,json",
    "legacyWatch": true,
    "exec": "node --inspect=0.0.0.0:9001 --trace-warnings src/index.js"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.1.1",
    "argon2": "^0.28.3",
    "config": "^3.3.6",
    "env-cmd": "^10.1.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.12",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "multer": "^1.4.4",
    "mysql2": "^2.3.2",
    "node-input-validator": "^4.5.0",
    "path": "^0.12.7",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "jest": "^29.2.2",
    "jest-each": "^29.2.1",
    "nodemon": "^2.0.14",
    "supertest": "^6.3.1"
  }
}
